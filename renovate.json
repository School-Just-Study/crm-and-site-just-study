{
  "baseBranches": [
    "develop"
  ],
  "extends": [
    "config:base"
  ],
  "schedule": "every weekend",
  "packageRules": [
    {
      "matchPackagePatterns": [
        "^@keystone"
      ],
      "groupName": "@keystonejs"
    },
    {
      "matchPackagePatterns": ["^@mui"],
      "groupName": "@mui"
    },
    {
      "matchPackagePatterns": [
        "^eslint"
      ],
      "groupName": "@ESLint"
    },
    {
      "matchPackagePatterns": [
        "^@types"
      ],
      "groupName": "@types"
    },
    {
      "matchPackagePatterns": [
        "^@semantic-release"
      ],
      "groupName": "@semantic-release"
    },
    {
      "matchPackagePatterns": [
        "^@fullcalendar"
      ],
      "groupName": "@fullcalendar"
    },
    {
      "matchPackagePatterns": [
        "^effector"
      ],
      "groupName": "@effector"
    },
    {
      "matchPackagePatterns": [
        "^@emotion"
      ],
      "groupName": "@emotion"
    },
    {
      "matchPackagePatterns": [
        "^@graphql-codegen"
      ],
      "groupName": "@graphql-codegen"
    }
  ]
}
